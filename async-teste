  import AsyncStorage from '@react-native-community/async-storage';
  
  const globalState = useContext(store);
  const {decks, setDecks, deckname, cards} = globalState;

  useEffect(() => {
    setData();
    saveData();
    // eslint-disable-next-line react-hooks/exhaustive-deps
  }, []);

  const setData = async () => {
    try {
      await AsyncStorage.setItem(
        'decks',
        setDecks(JSON.stringify({decks: {deckname, cards}})),
      );
    } catch (e) {
      console.warn(e);
    }
  };

  const saveData = async () => {
    try {
      await AsyncStorage.getItem('decks');
    } catch (e) {
      console.warn(e);
    }
  };

  console.warn('setDecks', setDecks);
  console.warn('decks', decks);
  console.warn('decks', deckname, cards);